@startuml
hide footbox
!theme plain
skinparam backgroundcolor transparent

' Turquoise #668c97
' Orange #F6921E

participant "Notebook\nAE" as AE 
box "CSE & Hosted Resources" #f8f8f8
participant "CSE" as CSE 
participant "container_1" as container_1 #F6921E
participant "container_2" as container_2 #F6921E
end box

AE -> CSE ++: RETRIEVE <containers>\ncse-in/Notebook-AE/Group/fopt
CSE -> container_1 ++: RETRIEVE <container>\ncse-in/Notebook-AE/container_1
CSE <- container_1 --: Response
CSE -> container_2 ++: CREATE <container>\ncse-in/Notebook-AE/container_2
CSE <- container_2 --: Response
AE <- CSE --: Response (Aggregated)

@enduml